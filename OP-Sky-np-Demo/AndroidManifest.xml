			<intent-filter>
				<action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
			</intent-filter>
		</receiver>
		<provider android:name="com.igexin.download.DownloadProvider" 
			android:process=":pushservice" 
			android:authorities="downloads.cn.stgame.p1.np"/>
